関数:実行() => () {
	配列<整数>: 素数;

	素数.追加(2);

	(整数：数 = 3;) から (数 > 100) まで (数 += 2;) {
		整数:フラグ = 0;
		(整数：インデックス = 0;) から (インデックス == 素数.長さ()) まで (インデックス++;) {
			もし ( (数 % 素数.取得(インデックス)) == 0 ) ならば {
				フラグ = 1;
			}
		}
		もし ( フラグ == 0 ) ならば {
			素数.追加(数);
		}
	}

	(整数：インデックス = 0;) から (インデックス == 素数.長さ()) まで (インデックス++;) {
		表示(素数.取得(インデックス));
	}
}